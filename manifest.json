{
  "manifest_version": 3,
  "name": "Prompt master",
  "description": "Managing prompts in ai",
  "version": "0.1.0",
  "permissions": ["scripting", "storage", "contextMenus"],
  "host_permissions": ["https://meet.google.com/*"],
  "action": {
    "default_popup": "dist/popup.html"
  },
  "icons": {
    "16": "public/bookmark_icon16.png",
    "48": "public/bookmark_icon48.png",
    "128": "public/bookmark_icon128.png"
  },
  "background": {
    "service_worker": "dist/background.js",
    "type": "module"
  }
}

