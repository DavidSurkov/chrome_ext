{
  "manifest_version": 3,
  "name": "Prompt master",
  "description": "Not decided yet",
  "version": "0.1.0",
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "storage",
    "desktopCapture"
  ],
  "host_permissions": [
    "https://meet.google.com/*"
  ],
  "action": {
    "default_popup": "dist/popup.html"
  },
  "background": {
    "service_worker": "dist/background.js",
    "type": "module"
  }
}